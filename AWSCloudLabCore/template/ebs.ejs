"LabStorageVolume<%-i%>":{
    "Type":"AWS::EC2::Volume",
    "DeletionPolicy":"Delete",
    "Properties":{
        "SnapshotId": "<%-user.labStorageSnapshotId%>",
        "AvailabilityZone":{
            "Fn::Select" : [ "0", { "Fn::GetAZs" : "" } ]
        },
        "Tags":
        [
            {
                "Key": "Owner",
                "Value": "<%-user.email%>"
            },
            {
                "Key": "AWSAccount",
                "Value": "<%-user.awsAccount%>"
            },
            {
                "Key": "Name",
                "Value": "BackupLabStorage-<%-user.email%>"
            }
        ]
    }
}
